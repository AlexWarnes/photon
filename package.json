		"eslint-plugin-svelte3": "^4.0.0",
		"lamina": "^1.1.20",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"rxjs": "^7.5.5",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-cubed": "^0.2.1",
